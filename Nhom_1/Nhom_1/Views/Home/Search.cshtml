@model IEnumerable<Nhom_1.Models.SanPham>

@{
    ViewBag.Title = "Tìm kiếm";
    IEnumerable<string> danhMucs = Model.OrderBy(dm => dm.DanhMucSP.Ten_DM).Select(dm => dm.DanhMucSP.Ten_DM).Distinct();
    IEnumerable<string> thuongHieus = Model.OrderBy(dm => dm.ThuongHieu_SP).Select(dm => dm.ThuongHieu_SP).Distinct();
    IEnumerable<string> gias = Model.OrderBy(dm => dm.GiaBan_SP).Select(dm => dm.GiaBan_SP.ToString()).Distinct();
    IEnumerable<string> mucDichs = Model.OrderBy(dm => dm.MucDich_SP).Select(dm => dm.MucDich_SP.ToString()).Distinct();
    IEnumerable<string> nongDos = Model.OrderBy(dm => dm.NongDo_SP).Select(dm => dm.NongDo_SP).Distinct();
    IEnumerable<string> muiHuongs = Model.OrderBy(dm => dm.MuiHuong_SP).Select(dm => dm.MuiHuong_SP).Distinct();
}

<div class="main">
    <ol itemscope="" itemtype="http://schema.org/BreadcrumbList">
        <li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem">
            <a itemprop="item" href="@Url.Action("Index", "Home")"><span itemprop="name" title="Trang chủ">Trang chủ</span></a> ❯ <meta itemprop="position" content="1">
        </li>
        <li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem">
            <a itemprop="item" href="@Url.Action("Search", "Home")" title="Tìm nước hoa theo nhu cầu"><span itemprop="name">Tìm nước hoa theo nhu cầu</span></a><meta itemprop="position" content="2">
        </li>
    </ol>
    <div class="topsearch">
        <div class="search">
            <form action="Search" method="post">
                <input type="search" placeholder="Từ khóa tìm kiếm" name="txtKey"/>
                <div class="clr10"></div>
                <select name="danhMuc">
                    <option value="0">Danh mục</option>
                    @{
                        foreach (string str in danhMucs)
                        {
                            <option value="@str">@str</option>
                        }
                    }
                </select>
                <select name="thuongHieu">
                    <option value="0">Thương hiệu</option>
                    @{
                        foreach (string str in thuongHieus)
                        {
                            <option value="@str">@str</option>
                        }
                    }
                </select>
                <select name="giaBan">
                    <option value="0">Khoảng giá</option>
                    @{
                        for (int j = 2000000; j <= 20000000; j = j + 2000000)
                        {
                            <option value="@j">@j VND</option>
                        }
                    }
                </select>
                <select name="mucDich">
                    <option value="0">Mục đích sử dụng</option>
                    @{
                        foreach (string str in mucDichs)
                        {
                            <option value="@str">@str</option>
                        }
                    }
                </select>
                <select name="nongDo">
                    <option value="0">Nồng độ</option>
                    @{
                        foreach (string str in nongDos)
                        {
                            <option value="@str">@str</option>
                        }
                    }
                </select>
                <select name="muiHuong">
                    <option value="0">Mùi hương</option>
                    @{
                        foreach (string str in muiHuongs)
                        {
                            <option value="@str">@str</option>
                        }
                    }
                </select>
                <button class="submit" type="submit"></button>
                <div class="clr10"></div>
            </form>
        </div>
    </div>
    <style type="text/css">
        .topsearch {
            width: 100%;
            margin: 20px 0;
        }

        .topsearch .main {
            background: rgba(255,255,255,0.95);
            border-radius: 3px;
        }

        .topsearch a.logo {
            display: block;
            width: 25%;
            float: left;
        }

        .topsearch a.logo img {
            max-width: 100%;
            max-height: 60px;
        }

        .topsearch .search {
            width: 100%;
        }

        .topsearch .search {
            height: 34px;
            line-height: 34px;
        }

        .topsearch .search input {
            height: 32px;
            line-height: 32px;
            width: 100%;
            padding: 0 5px;
            border: 0.5px solid #ccc;
            background: #fff;
            display: block;
            float: left;
            text-align: left;
            border-radius: 0;
        }

        .topsearch .search select {
            height: 32px;
            line-height: 32px;
            width: 16.66%;
            padding: 0 2px;
            border: 0.5px solid #ccc;
            background: #fff;
            display: block;
            float: left;
        }

        .topsearch .search button.submit {
            height: 32px;
            line-height: 32px;
            width: 10%;
            padding: 0 10px;
            display: block;
            float: left;
            border-radius: 0;
            cursor: pointer;
            background: #999 url(../../wwwroot/images/icons/search74.png) center center no-repeat;
            background-size: 20px;
            margin-top: 20px;
        }

        .topsearch .search button.submit:hover {
            background-color: #7f8fa6;
        }
    </style>
    <div class="content">
